<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 数据库驱动包位置 -->
    <classPathEntry location="./lib/mysql-connector-java-5.1.5-bin.jar"/>

    <context id="context1" defaultModelType="flat">

        <!-- 生成的pojo，将implements Serializable-->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <!-- 去掉生成注释 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
            <property name="addRemarkComments" value="true"/>
        </commentGenerator>


        <!-- 数据库链接URL、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/spring?useUnicode=true&amp;characterEncoding=utf8&amp;zeroDateTimeBehavior=convertToNull"
                        userId="root"
                        password="root"/>

        <!-- info_表 -->
        <javaModelGenerator targetPackage="com.free.system.id.model" targetProject="generate-source"/>
        <sqlMapGenerator targetPackage="com.free.system.id.mapper" targetProject="generate-source"/><!-- XML映射文件 -->
        <javaClientGenerator targetPackage="com.free.system.id.mapper" targetProject="generate-source" type="XMLMAPPER"/><!-- Mapper文件 -->
        <javaServiceGenerator targetPackage="com.free.system.id.service" targetProject="generate-source"/>
<!--         <javaControllerGenerator targetPackage="yiyou.tt.commision.core.modules.commision.controller" targetProject="generate-source"/> -->

        <!-- 要生成哪些表(更改tableName和domainObjectName就可以) -->
       <!-- <table tableName="ad_setting" domainObjectName="Setting"
                 enableCountByExample="false"
                 enableUpdateByExample="false" enableDeleteByExample="false"
                 enableSelectByExample="false" selectByExampleQueryId="false"/> -->

		<table tableName="unique_id_log" domainObjectName="UniqueIdLog"
					 enableCountByExample="false"
					 enableUpdateByExample="false" enableDeleteByExample="false"
					 enableSelectByExample="false" selectByExampleQueryId="false"/>

    </context>
</generatorConfiguration>
